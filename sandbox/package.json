{
  "name": "@diesel-parser/json-monaco",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@diesel-parser/monaco": "file:../monaco",
    "@diesel-parser/samples": "~0.15.0",
    "monaco-editor": "^0.34.1",
    "monaco-editor-workers": "0.34.2",
    "monaco-languageclient": "4.0.2",
    "vscode-json-languageservice": "5.1.3"
  },
  "devDependencies": {
    "@diesel-parser/ts-facade": "~0.15.0",
    "@types/vscode": "~1.69.1",
    "path": "^0.12.7",
    "path-browserify": "~1.0.1",
    "shx": "~0.3.4",
    "tslib": "2.4.1",
    "vite": "3.2.4"
  },
  "volta": {
    "node": "16.18.1",
    "npm": "8.19.3"
  },
  "scripts": {
    "clean": "npx shx rm -fr dist tsconfig.tsbuildinfo",
    "lint": "eslint src --ext .ts",
    "compile": "tsc",
    "watch": "tsc -w",
    "copy:monacoworkers": "shx mkdir -p dist && shx cp -r ../node_modules/monaco-editor-workers/dist/workers/editorWorker* ./dist",
    "build": "npm run clean && npm run compile && npm run copy:monacoworkers",
    "dev": "vite"
  }
}
